;;
;; settings using customize interface
;;
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(company-selection-wrap-around t)
 '(custom-enabled-themes '(tsdh-dark))
 '(epa-pinentry-mode 'loopback)
 '(epg-pinentry-mode 'loopback)
 '(eww-search-prefix "https://google.com/search?q=")
 '(global-font-lock-mode t)
 '(inhibit-startup-screen t)
 '(js-indent-level 2)
 '(package-selected-packages
   '(eglot yaml-mode web-mode smartrep projectile magit hydra helm-ag helm editorconfig docker csv-mode company ##))
 '(python-indent-guess-indent-offset-verbose nil)
 '(recentf-max-saved-items 50)
 '(recentf-mode t)
 '(size-indication-mode t)
 '(xterm-mouse-mode nil))


(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(whitespace-space ((t (:foreground "cyan"))))
 '(whitespace-tab ((t (:foreground "cyan")))))
